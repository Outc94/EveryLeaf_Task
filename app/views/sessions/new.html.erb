<div class="main-form border">
  <h5 class="text-center">Log In to keep up with your Deadlines.</h5>
  <%= form_with(scope: :session, url: sessions_path, local: true) do |form| %>

    <div class="form-group">
    <%= form.email_field :email, placeholder:"email", class:"form-control" %>
    </div>

    <div class="form-group">
    <%= form.password_field :password, placeholder:"Password", class:"form-control" %>
    </div>

    <div class="form-group">
    <%= form.submit "Log in", class:"form-control btn btn-info" %>
    </div>
  <% end %>
</div>
<div class="main-form border">
  <label>Don't have an account? <a><%= link_to "Sign up", new_user_path %><a></label>
</div>
